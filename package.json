{
  "name": "attendee-to-attendee-widget",
  "version": "3.1.1-beta.0",
  "description": "Real time attendees tracking widget",
  "author": "romanetar",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fntechgit/attendee-to-attendee-widget.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "source": "src/index.js",
  "scripts": {
    "clean": "find . -name \"dist\" -type d -prune -exec rm -rf '{}' + & find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' + && yarn",
    "build-dev": "./node_modules/.bin/webpack --config webpack.dev.js",
    "build": "./node_modules/.bin/webpack --config webpack.prod.js && yarn copy-extras",
    "serve": "webpack-dev-server --open --port=8888 --https --config webpack.dev.js",
    "copy-extras": "cp -rf package.json README.md dist",
    "publish-package": "yarn build && yarn publish",
    "test": "jest --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!react-dnd|dnd-core|@react-dnd)"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/helper-string-parser": "^7.19.4",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@react-pdf/renderer": "^3.1.12",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.4",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "browser-tabs-lock": "^1.2.15",
    "clean-webpack-plugin": "^4.0.0",
    "crypto-js": "^4.1.1",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "file-loader": "^6.2.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^5.5.0",
    "i18n-react": "^0.6.4",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^2.6.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "node-sass": "^7.0.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "path": "^0.12.7",
    "react": "^16.8.4",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.35.6",
    "bulma": "^0.9.0",
    "formik": "^2.2.6",
    "lodash.debounce": "^4.0.8",
    "luxon": "^1.26.0",
    "public-ip": "^4.0.3",
    "react-device-detect": "^2.0.1",
    "react-error-boundary": "^3.1.3",
    "react-infinite-scroll-component": "^6.0.0",
    "react-markdown": "^6.0.2",
    "react-router-dom": "^5.3.4",
    "react-tooltip": "^4.2.19",
    "rehype-raw": "^5.1.0",
    "stream-chat": "4.2.0",
    "stream-chat-react": "4.1.3",
    "yup": "^0.32.9"
  }
}
